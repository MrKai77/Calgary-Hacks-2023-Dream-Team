<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="456dp"
        android:layout_height="44dp"
        android:orientation="horizontal"
        app:barrierAllowsGoneWidgets="false"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout">

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="96dp"
            android:layout_height="44dp"
            android:text="Parking Spots: " />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="448dp"
        android:layout_height="79dp"
        android:layout_marginBottom="68dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <Button
            android:id="@+id/theRefreshButton"
            android:layout_width="wrap_content"
            android:layout_height="67dp"
            android:layout_marginBottom="108dp"
            android:text="Refresh" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />
    </LinearLayout>

    <ImageView
        android:id="@+id/mruTemplate"
        android:layout_width="416dp"
        android:layout_height="347dp"
        app:srcCompat="@drawable/campus_parking_map"
        tools:layout_editor_absoluteY="64dp" />

    <ImageView
        android:id="@+id/eastGateParkade"
        android:layout_width="344dp"
        android:layout_height="529dp"
        app:layout_constraintBottom_toBottomOf="@+id/mruTemplate"
        app:layout_constraintEnd_toEndOf="@+id/mruTemplate"
        app:layout_constraintStart_toStartOf="@+id/mruTemplate"
        app:layout_constraintTop_toTopOf="@+id/mruTemplate"
        app:srcCompat="@drawable/eastgateparkade_green" />

    <ImageView
        android:id="@+id/parkingLotB"
        android:layout_width="413dp"
        android:layout_height="586dp"
        app:layout_constraintBottom_toBottomOf="@+id/mruTemplate"
        app:layout_constraintEnd_toEndOf="@+id/mruTemplate"
        app:layout_constraintStart_toStartOf="@+id/mruTemplate"
        app:layout_constraintTop_toTopOf="@+id/mruTemplate"
        app:srcCompat="@drawable/lotb_green" />

    <ImageView
        android:id="@+id/parkingLot4"
        android:layout_width="411dp"
        android:layout_height="555dp"
        app:layout_constraintBottom_toBottomOf="@+id/mruTemplate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/mruTemplate"
        app:layout_constraintTop_toTopOf="@+id/mruTemplate"
        app:srcCompat="@drawable/lot4_green" />

    <ImageView
        android:id="@+id/parkingLot5"
        android:layout_width="415dp"
        android:layout_height="544dp"
        app:layout_constraintBottom_toBottomOf="@+id/mruTemplate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/mruTemplate"
        app:layout_constraintTop_toTopOf="@+id/mruTemplate"
        app:srcCompat="@drawable/lot5_green" />

